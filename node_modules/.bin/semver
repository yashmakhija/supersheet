#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chiragmakhija/Yashu/super-30/monorepo-starter/apps/http-backend/node_modules/semver/bin/node_modules:/Users/chiragmakhija/Yashu/super-30/monorepo-starter/apps/http-backend/node_modules/semver/node_modules:/Users/chiragmakhija/Yashu/super-30/monorepo-starter/apps/http-backend/node_modules:/Users/chiragmakhija/Yashu/super-30/monorepo-starter/apps/node_modules:/Users/chiragmakhija/Yashu/super-30/monorepo-starter/node_modules:/Users/chiragmakhija/Yashu/super-30/node_modules:/Users/chiragmakhija/Yashu/node_modules:/Users/chiragmakhija/node_modules:/Users/node_modules:/node_modules:/Users/chiragmakhija/Yashu/super-30/monorepo-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chiragmakhija/Yashu/super-30/monorepo-starter/apps/http-backend/node_modules/semver/bin/node_modules:/Users/chiragmakhija/Yashu/super-30/monorepo-starter/apps/http-backend/node_modules/semver/node_modules:/Users/chiragmakhija/Yashu/super-30/monorepo-starter/apps/http-backend/node_modules:/Users/chiragmakhija/Yashu/super-30/monorepo-starter/apps/node_modules:/Users/chiragmakhija/Yashu/super-30/monorepo-starter/node_modules:/Users/chiragmakhija/Yashu/super-30/node_modules:/Users/chiragmakhija/Yashu/node_modules:/Users/chiragmakhija/node_modules:/Users/node_modules:/node_modules:/Users/chiragmakhija/Yashu/super-30/monorepo-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
